                <item>
                    <uuid>a1b16c82463c486e92b22e88d4358ac8</uuid>
                    <name>Get Gov weather</name>
                    <type>HTTP_AGENT</type>
                    <key>get_weather.http</key>
                    <delay>10m</delay>
                    <history>1d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <timeout>10s</timeout>
                    <url>https://api.weather.gov/stations/{$STATION}/observations/latest</url>
                    <query_fields>
                        <query_field>
                            <name>require_qc</name>
                            <value>false</value>
                        </query_field>
                    </query_fields>
                    <headers>
                        <header>
                            <name>User-Agent</name>
                            <value>ChangeMe!  Change Me!</value>
                        </header>
                    </headers>
                    <tags>
                        <tag>
                            <tag>Application</tag>
                            <value>GovWeather</value>
                        </tag>
                    </tags>
                </item>
